{
  "name": "imagemin-lint-staged",
  "version": "0.3.0",
  "description":
    "imagemin CLI designed for lint-staged usage with sensible defaults",
  "license": "MIT",
  "author": {
    "name": "tomchentw",
    "email": "developer@tomchentw.com",
    "url": "https://github.com/tomchentw"
  },
  "contributors": [{
    "name": "CÃ©sar Valadez",
    "email": "cesar@wdinamo.com",
    "url": "https://dinamo.mx"
  }],
  "repository": {
    "type": "git",
    "url": "https://github.com/tomchentw/imagemin-lint-staged"
  },
  "bugs": {
    "url": "https://github.com/tomchentw/imagemin-lint-staged/issues"
  },
  "homepage": "https://github.com/tomchentw/imagemin-lint-staged/",
  "bin": "bin/index.js",
  "files": [
    "bin/",
    "lib/",
    "src/",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "imagemin",
    "CLI",
    "git hook",
    "compress images",
    "lint-staged"
  ],
  "scripts": {
    "test": "jest --runInBand",
    "prebuild:lib": "rimraf lib",
    "build:lib": "babel-multi-env --multi-versions 8.0.0 6.0.0 --out-dir lib --given 'src/**/*.js'",
    "commit:lib": "git add -A && git commit -m 'chore(lib): compile from src with `babel-multi-env`'",
    "prerelease": "npm run commit:lib",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ],
    "*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "env"
          ]
        ]
      }
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "\\d+\\.\\d+\\.\\d+"
    ]
  },
  "husky": {
    "pre-commit": "lint-staged",
    "pre-commit:lib": "npm run build:lib"
  },
  "dependencies": {
    "babel-runtime": "^7.0.0-beta.3",
    "imagemin-gifsicle": "^5.2.0",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-optipng": "^5.2.1",
    "imagemin-svgo": "^7.0.0",
    "semver": "^5.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.2",
    "babel-multi-env": "^0.6.1",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "babel-plugin-transform-runtime": "^7.0.0-beta.3",
    "babel-preset-env": "^7.0.0-beta.3",
    "husky": "^1.0.0-rc.13",
    "jest": "^21.3.0-beta.5",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "standard-version": "^4.4.0"
  }
}
